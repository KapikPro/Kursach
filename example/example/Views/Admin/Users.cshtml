@{
    ViewData["Title"] = "Users";
}

<div class="text-center">
    <h1 class="display-4">Users</h1>
</div>
</div>

<br />
<hr />
<br />

<table cellpadding="10">
    <tr>
        <th>Id</th>
        <th>Email</th>
        <th>Дата создания</th>

        <th></th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Email</td>
            <td>@item.Date.ToString("yyyy.MM.dd HH:mm")</td>
            @* <td><a href="/admin/users/block/@item.Id">Block</a></td>
            <td><a href="/admin/users/unblock/@item.Id">Unblock</a></td> *@
        </tr>
    }
</table>